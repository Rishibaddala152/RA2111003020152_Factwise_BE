def maxScore(cardPoints,k):
    n=len(cardPoints)

    current_sum=sum(cardPoints[-k:])
    max_sum=max(max_sum,current_sum)

    for i in range(i,k+1):
        current_sum=current_sum-cardPoints[-i]+cardPoints[k-i]
        max_sum=max(max_sum,current_sum)

    return max_sum

cardPoints=eval(input())
k=int(input())
print(maxScore(cardPoints,k))
